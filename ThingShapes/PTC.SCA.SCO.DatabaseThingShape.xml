                <ServiceImplementation
                 description=""
                 handlerName="Script"
                 name="GetEntity">
                    <ConfigurationTables>
                        <ConfigurationTable
                         description="Script"
                         isMultiRow="false"
                         name="Script"
                         ordinal="0">
                            <DataShape>
                                <FieldDefinitions>
                                    <FieldDefinition
                                     baseType="STRING"
                                     description="code"
                                     name="code"
                                     ordinal="0"></FieldDefinition>
                                </FieldDefinitions>
                            </DataShape>
                            <Rows>
                                <Row>
                                    <code>
                                    <![CDATA[
                                    var params = {
                                    	dataShapeName: dataShapeName
                                    };
                                    
                                    var primaryKey = me.GetPrimaryKey(params);
                                    
                                    var filter = {
                                        filters:{
                                            fieldName:primaryKey,
                                            type:"EQ",
                                            value:UID
                                        }
                                    };
                                    
                                    var params = {
                                    	filter: filter,
                                    	dataShapeName: dataShapeName,
                                        limit: 1
                                    };
                                    
                                    var result = me.Query(params);
                                    ]]>
                                    </code>
                                </Row>
                            </Rows>
                        </ConfigurationTable>
                    </ConfigurationTables>
                </ServiceImplementation>
                <ServiceImplementation
                 description=""
                 handlerName="Script"
                 name="GetPrimaryKey">
                    <ConfigurationTables>
                        <ConfigurationTable
                         description="Script"
                         isMultiRow="false"
                         name="Script"
                         ordinal="0">
                            <DataShape>
                                <FieldDefinitions>
                                    <FieldDefinition
                                     baseType="STRING"
                                     description="code"
                                     name="code"
                                     ordinal="0"></FieldDefinition>
                                </FieldDefinitions>
                            </DataShape>
                            <Rows>
                                <Row>
                                    <code>
                                    <![CDATA[
                                    var fieldDefinitions = DataShapes[dataShapeName].GetFieldDefinitions();
                                    
                                    var length = fieldDefinitions.length;
                                    
                                    var result = undefined;
                                    
                                    for (var x = 0; x < length; x++) {
                                    	var fieldDefinition = fieldDefinitions[x];
                                    	if(fieldDefinition.isPrimaryKey){
                                            result = fieldDefinition.name;
                                        }
                                    }
                                    ]]>
                                    </code>
                                </Row>
                            </Rows>
                        </ConfigurationTable>
                    </ConfigurationTables>
