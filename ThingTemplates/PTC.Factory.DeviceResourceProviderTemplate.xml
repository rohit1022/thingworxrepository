                                    <Row>
                                        <code>
                                        <![CDATA[
                                        /*
                                            Prepare infotable to return
                                        */
                                        var result = Resources["InfoTableFunctions"].CreateInfoTableFromDataShape({
                                            infoTableName : "InfoTable",
                                            dataShapeName : "PTC.Resource.ListItem"
                                        });
                                        /*
                                            Get all device instance things by calling GET service
                                        */
                                        var allDeviceInstance =  me.Get({});
                                        /*
                                            Copy infomation of each retrieved device thing into result infotable
                                        */
                                        if (allDeviceInstance && allDeviceInstance.getRowCount() > 0) {
                                            for (var i = 0; i < allDeviceInstance.getRowCount(); i++) {
                                                result.AddRow({
                                                    label: allDeviceInstance.getRow(i).Name,
                                                    value: allDeviceInstance.getRow(i).Name
                                                });
                                            }
                                        }
                                        ]]>
                                        </code>
                                    </Row>
                                </Rows>
                            </ConfigurationTable>
                        </ConfigurationTables>
                    </ServiceImplementation>
